RABBITMQ_URL={{ rabbitmq_url }}
DATABASE_URL={{ vx_db_url }}

{% if github_key and github_secret %}
GITHUB_KEY={{ github_key }}
GITHUB_SECRET={{ github_secret }}
{% endif %}

VX_WEB_SECRET={{ vx_web_secret }}

{% if vx_web_hostname %}
VX_WEB_HOSTNAME={{ vx_web_hostname }}
{% endif %}

{% if not vx_web_signup %}
VX_WEB_DISABLE_SIGNUP=1
{% endif %}

{% if vx_web_use_ssl %}
VX_WEB_HTTPS=1
{% endif %}
